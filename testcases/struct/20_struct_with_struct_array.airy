struct Point { x: i32, y: i32 }

struct Triangle { vertices: [struct Point; 3] }

fn main() -> i32 {
  let t: struct Triangle;
  t.vertices[0].x = 0;
  t.vertices[0].y = 0;
  t.vertices[1].x = 10;
  t.vertices[1].y = 0;
  t.vertices[2].x = 5;
  t.vertices[2].y = 10;

  let sum: i32 = 0;
  let i: i32 = 0;
  while (i < 3) {
    sum = sum + t.vertices[i].x + t.vertices[i].y;
    i = i + 1;
  }
  return sum;
}
