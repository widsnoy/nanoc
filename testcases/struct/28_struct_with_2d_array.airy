struct Matrix { data: [[i32; 3]; 2], rows: i32, cols: i32 }

fn main() -> i32 {
  let m: struct Matrix;
  m.rows = 2;
  m.cols = 3;

  let i: i32 = 0;
  while (i < 2) {
    let j: i32 = 0;
    while (j < 3) {
      m.data[i][j] = i * 3 + j + 1;
      j = j + 1;
    }
    i = i + 1;
  }

  let sum: i32 = 0;
  i = 0;
  while (i < m.rows) {
    let j: i32 = 0;
    while (j < m.cols) {
      sum = sum + m.data[i][j];
      j = j + 1;
    }
    i = i + 1;
  }
  return sum;
}
