let ga: [i32; 10];
let gb: [[i32; 3]; 2] = {5, 6, 7, 8};

fn main() -> i32 {
  let a: [i32; 10];
  let b: [[i32; 3]; 2] = {1, 2, 3, 4};
  let sum: i32 = 0;
  let i: i32 = 0;
  while (i < 2) {
    let j: i32 = 0;
    while (j < 3) {
      sum = sum + b[i][j] + gb[i][j];
      j = j + 1;
    }
    i = i + 1;
  }
  i = 0;
  while (i < 10) {
    a[i] = sum + i;
    ga[i] = sum + i;
    i = i + 1;
  }
  return sum;
}
