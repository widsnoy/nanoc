fn main() -> i32 {
  let a: i32 = 0;
  let b: const i32 = 1 - 1 * 2 + 1;
  let c: i32 = 1;
  let d: i32 = 2;
  if (a || b) {
    c = 3;
  } else {
    d = 3;
    let a: i32 = 1;
    if (a || b) {
      c = 4;
    } else {
      d = 4;
    }
    if (a == 0) return 1;
    else if (a == 0 && a == -1) return 2;
  }
  return a + b + c + d;
}
