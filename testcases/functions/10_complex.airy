fn printf(t: *const u8, ...);

let a: i32 = 10;

fn inc() -> i32 {
  a = a + 1;
  return a;
}

fn print_a() {
  printf("%d", a);
  printf("%c", 10);
}

fn main() -> i32 {
  let i: i32 = 0;
  while (i < 10) {
    inc();
    let a: i32 = 1;
    a = a + 2;
    printf("%d", a);
    printf("%c", 10);
    print_a();
    i = i + 1;
  }
  return 0;
}
