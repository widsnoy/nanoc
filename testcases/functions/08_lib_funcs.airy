fn scanf(format: *const u8, ...);
fn printf(t: *const u8, ...);

fn __airyc_getint() -> i32 {
    let tmp: i32 = 0;
    scanf("%d", &tmp);
    return tmp;
}

fn __airyc_getch() -> i32 {
    let tmp: i32 = 0;
    scanf("%c", &tmp);
    return tmp;
}

fn main() -> i32 {
  let x: i32 = __airyc_getint();
  let y: i32 = __airyc_getint();
  printf("%d", x + y);
  let c: i32 = __airyc_getch();
  printf("%c", c);
  printf("%c", 33);
  printf("%c", 10);
  return 0;
}
