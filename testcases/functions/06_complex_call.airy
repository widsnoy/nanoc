fn add(a: i32, b: i32) -> i32 {
  return a + b;
}

fn sub(a: i32, b: i32) -> i32 {
  return a - b;
}

fn mul(a: i32, b: i32) -> i32 {
  return a * b;
}

fn div(a: i32, b: i32) -> i32 {
  return a / b;
}

fn main() -> i32 {
  let x: i32 = add(sub(1, 2), mul(3, div(4, 5)));
  let y: i32 = add(1 || 0, 0 && sub(1, x) || mul(3, div(x || add(1, 2) > 10, 5)));
  return x + y;
}
